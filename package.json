  "scripts": {
    "dev": "next dev",
    "dev:turbo": "nodemon --watch next.config.js --watch package.json --exec 'next dev --turbo'",
    "dev:performance": "NEXT_RUNTIME_OPTIMIZE=1 NODE_OPTIONS='--max_old_space_size=4096' next dev --turbo",
    "build": "next build",
    "build:performance": "ANALYZE=true NODE_ENV=production next build",
    "start": "next start",
    "start:performance": "NODE_ENV=production next start",
